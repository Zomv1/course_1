#include <stdlib.h>
#include <math.h>
#include <stdio.h>
#include <string.h>

void main() {
	int a[1000], i = 0, j = 0, kol = 0, maxkol, maxnum;
	double num;
	do {
		printf("\nВведите элемент массива - %i", i);
		scanf_s("%lf", $num);
		a[i] = num;
		i++;
	}while (num == float(num))

	int b[i - 1];
	for (i = i-1; i > 0; i--) {
		b[j] = a[i];
		j++;
	}
	b = xor(b);

	for (i = 0; i < j - 1; i++) {
		kol = 0;
		while (b[i] == b[i + 1] && i < j - 1) {
			kol++;
			if (kol > maxkol) {
				maxkol = kol;
				maxnum = b[i];
			}
		}
	}

	printf("\n Самое больше число %i встретилось %i раз.", maxnum, naxkol);
	return;
}
